{
  "task": "Grammar Checker",
  "output_directory": "../artefacts/",
  "data_path": "../data/in_domain_dev.tsv",
  "logging_steps": 10,
  "n_iterations": 4,
  "train_batch_size": 32,
  "test_batch_size": 32,
  "weight_decay": 0.1,
  "train_split": 0.85,
  "label_column": "label",
  "text_column": "sentence",
  "learning_rate": 0.0003,
  "sentence_transformer": "distiluse-base-multilingual-cased-v2",
  "dropout": 0.001,
  "embedding_size": 512,
  "separator": "\t",
  "cuda": false
}